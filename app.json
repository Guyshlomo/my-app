{
  "expo": {
    "name": "Voluntree",
    "slug": "voluntree",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appicon.png",
    "scheme": "voluntree",
    "userInterfaceStyle": "automatic",
    "description": "A volunteer management and community engagement platform connecting volunteers with local organizations. Minimum age requirement: 17 years.",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "deviceFamily": [
        "1",
        "2"
      ],
      "bundleIdentifier": "com.voluntree.myapp",
      "buildNumber": "27",
      "infoPlist": {
        "UIDeviceFamily": [
          1,
          2
        ],
        "LSRequiresIPhoneOS": true,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSAllowsLocalNetworking": true,
          "NSExceptionDomains": {
            "oomibleqeelsswfbkjou.supabase.co": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSExceptionMinimumTLSVersion": "TLSv1.2",
              "NSExceptionRequiresForwardSecrecy": true,
              "NSIncludesSubdomains": true
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "voluntree-oauth",
            "CFBundleURLSchemes": ["voluntree"]
          }
        ],
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "package": "com.voluntree.myapp",
      "versionCode": 4,
      "supportsTablet": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "voluntree"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/shaarhanegev.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "defaultChannel": "volunteer-events"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./assets/images/shaarhanegev.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fec72c28-8706-4ed2-81ef-9d1a28a23345"
      }
    },
    "owner": "guyshlomo"
  }
}
